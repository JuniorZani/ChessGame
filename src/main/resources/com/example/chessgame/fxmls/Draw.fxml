    <Text layoutX="121.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ocorreu um empate">
        <font>
            <Font size="15.0"/>
        </font>
    </Text>
    <Text fx:id="drawReason" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER"
